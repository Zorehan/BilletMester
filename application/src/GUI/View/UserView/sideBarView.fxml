<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="sidePane" prefHeight="700.0" prefWidth="200.0" stylesheets="@../../Styling/UserView.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controller.UserView.sideBarController">
   <children>
      <VBox>
         <children>
            <VBox alignment="TOP_CENTER" prefHeight="616.0" prefWidth="200.0" spacing="20.0">
               <children>
                  <Button fx:id="btnAccount" mnemonicParsing="false" onAction="#clickAccount" prefHeight="50.0" prefWidth="175.0" text="Account Settings" />
                  <Button fx:id="btnTickets" mnemonicParsing="false" onAction="#clickTickets" prefHeight="50.0" prefWidth="175.0" text="My Tickets" />
               </children>
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </VBox>
            <VBox alignment="BOTTOM_CENTER" prefHeight="89.0" prefWidth="200.0">
               <children>
                  <Button fx:id="btnLogOut" mnemonicParsing="false" onAction="#clickLogout" prefHeight="50.0" prefWidth="175.0" text="Log Out" />
               </children>
            </VBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
