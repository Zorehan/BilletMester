<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="600.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controller.UserView.sideBarController">
   <children>
      <VBox prefHeight="600.0" prefWidth="200.0">
         <children>
            <Button fx:id="btnAccount" mnemonicParsing="false" onAction="#clickAccount" prefHeight="85.0" prefWidth="200.0" text="Account" />
            <Button fx:id="btnTickets" mnemonicParsing="false" onAction="#clickTickets" prefHeight="85.0" prefWidth="200.0" text="Tickets" />
            <Button fx:id="btnLogOut" mnemonicParsing="false" onAction="#clickLogout" prefHeight="85.0" prefWidth="200.0" text="Log Out" />
         </children>
      </VBox>
   </children>
</AnchorPane>
